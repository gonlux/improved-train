<p id="notice"><%= notice %></p>

<h1>Weblog New Publication</h1>

    <% @posts.each do |post| %>
      
       <%= post.title %><br> 
       <%= post.description %><br>
       <%= post.created_at %><br><br>
       <%= link_to 'Show', post %> | |
       <%= link_to 'Edit', edit_post_path(post) %> | |
       <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %> | |
  
<br><br>
<%= link_to 'New Post', new_post_path %>

<% end %>
